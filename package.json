{
  "name": "pexeso",
  "version": "0.0.1",
  "description": "A memory card game that will test your wits! How many rounds can you complete in 60 seconds?! Remember each round is harder than the previous.",
  "main": "server.js",
  "scripts": {
    "start": "gulp",
    "first-time-setup": "node first-time-setup.js",
    "test": "lab -t 99 -S -T ./test/lab/transform -L --lint-options '{\"extensions\":[\".js\",\".jsx\"]}' ./test/lab/client-before.js ./test/client/ ./test/lab/client-after.js ./test/server/ ./test/lab/server-after.js ./test/misc/",
    "test-client": "lab -v -t 99 -S -T ./test/lab/transform ./test/lab/client-before.js $TEST_TARGET ./test/lab/client-after.js || true",
    "test-server": "lab -v -t 99 -S -T ./test/lab/transform $TEST_TARGET ./test/lab/server-after.js || true",
    "postinstall": "gulp build"
  },
